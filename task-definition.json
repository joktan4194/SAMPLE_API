{
  "family": "dcdojo",
  "executionRoleArn": "arn:aws:iam::573096830291:role/ecsTaskExecutionRole",
  "containerDefinitions": [
      {
          "name": "dcdojo-container",
          "image": "573096830291.dkr.ecr.us-east-1.amazonaws.com/simple-user-api",
          "cpu": 512,
          "portMappings": [
            {
              "hostPort": 8000,
              "protocol": "tcp",
              "containerPort": 8000
            }
          ],
          "memory": 1024,
          "essential": true,
          "logConfiguration": {
            "logDriver": "awslogs",
            "options": {
              "awslogs-group": "/ecs/joktanlog",
              "awslogs-region": "us-east-1",
              "awslogs-stream-prefix": "ecs"
            }
          }
      }
  ],
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "networkMode": "awsvpc",
  "cpu": "1024",
  "memory": "2048"
}
